{
    "name": "uunmask2000_kk/coin_currency",
    "authors": [
        {
            "name": "uunmask2000_kk",
            "email": "uunmask2000@gmail.com",
            "homepage": "https://twitter.com/uunmask2000",
            "role": "Developer"
        }
    ],
    "description": "提供法定(虛擬)貨幣api",
    "type": "library",
    "license": "LGPL-3.0",
    "minimum-stability": "stable",
    "support": {
        "issues": "https://github.com/uunmask2000/assets_country/issues"
    },
    "require": {
        "php": ">=7.2",
        "ext-simplexml": "*",
        "ext-xml": "*",
        "guzzlehttp/guzzle": "7.5.0"
    },
    "require-dev": {
        "ext-dom": "*",
        "ext-zip": "*",
        "phpunit/phpunit": "9.5.27"
    },
    "suggest": {
        "ext-zip": "Needed to store or download multiple files at once."
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "src/"
        ]
    },
    "extra": {
        "branch-alias": {}
    }
}
